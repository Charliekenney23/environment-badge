      "@dosomething/babel-config"
    ],
    "plugins": [
      "transform-inline-environment-variables"
    ]
  },
  "author": "David Furnes <dfurnes@dosomething.org>",
  "license": "MIT",
  "devDependencies": {
    "@dosomething/babel-config": "^2.2.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3"
  }
}
